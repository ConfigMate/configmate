{
    "server": {
        "host": "localhost",
        "port": 8080,
        "ssl_enabled": false,
        "ssl_cert_path": "/path/to/cert.pem",
        "ssl_key_path": "/path/to/key.pem"
    },
    "database": {
        "host": "db.example.com",
        "port": 5432,
        "username": "dbuser",
        "password": "dbpassword",
        "name": "dbname"
    },
    "cache": {
        "host": "cache.example.com",
        "port": 6379
    },
    "api": {
        "endpoint": "/api/v1",
        "timeout": 30
    },
    "logging": {
        "level": "info",
        "output": "logfile.log"
    }
}